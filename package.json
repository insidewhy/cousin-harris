{
  "name": "cousin-harris",
  "version": "3.0.1",
  "description": "a wrapper around fb-watchman with a nicer interface",
  "author": "insidewhy <github@chilon.net>",
  "license": "ISC",
  "keywords": [
    "typescript",
    "tsc"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/insidewhy/cousin-harris.git"
  },
  "main": "dist/index.js",
  "typings": "dist/index",
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "yarn build",
    "test": "jest",
    "build": "tsc -p src",
    "typecheck": "yarn build --noEmit",
    "watch": "concurrently \"yarn build -w\" \"yarn test --watchAll\"",
    "clean": "rimraf dist",
    "lint": "eslint 'src/*.ts'",
    "lint-fix": "yarn lint --fix",
    "run-prettier": "prettier 'src/*.ts' '*.md'",
    "validate-prettiness": "yarn run-prettier -c",
    "make-prettier": "yarn run-prettier --write",
    "validate": "yarn test && yarn lint && yarn validate-prettiness"
  },
  "dependencies": {
    "fb-watchman": "^2.0.1",
    "fs-extra": "^9.0.0"
  },
  "devDependencies": {
    "@types/fb-watchman": "2.0.0",
    "@types/fs-extra": "9.0.6",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.176",
    "@types/node": "12.20.55",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "concurrently": "6.3.0",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "lodash": "4.17.21",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "rimraf": "3.0.2",
    "typescript": "4.2.2",
    "wait-for-expect": "3.0.2"
  },
  "jest": {
    "testRegex": "\\.spec\\.js$"
  }
}
